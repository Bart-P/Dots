# Navigation
def_key "j"
  scroll_down
def_key "k"
  scroll_up
def_key "h"
  previous_column
def_key "l"
  next_column
def_key "g"
  move_home
def_key "G"
  move_end

# Search (basic)
def_key "/"
  find_item_forward
def_key "n"
  find_item_forward
def_key "N"
  find_item_backward

# Playlist / items
def_key "d"
  delete_playlist_items
def_key "y"
  select_item
def_key "p"
  add_selected_items

# Pause
def_key " "
  pause

# Volume
def_key "+"
  volume_up
def_key "-"
  volume_down

# Quit
def_key "q"
  quit
